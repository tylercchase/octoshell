{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}",
            "${workspaceFolder}/include"
        ],
        "myCompilerPath": "/usr/local/bin/gcc-7"
    },
    "configurations": [
        {
            "name": "Mac",
            "intelliSenseMode": "${default}",
            "defines": [
                "FOO",
                "BAR=100"
            ],
            "compilerPath": "/usr/bin/clang++-10",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "includePath": [
                "${default}",
                "${workspaceFolder}/build/_deps/catch2-src/single_include"
            ]
        }
    ],
    "version": 4
}